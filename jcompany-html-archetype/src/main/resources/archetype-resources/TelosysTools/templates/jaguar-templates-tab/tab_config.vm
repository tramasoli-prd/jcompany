#set( $uncapitalizedEntityName = $fn.uncapitalize($entity.name) )
#set( $dollarSymbol= "$" )

(function() {
	'use strict';

	angular
	.module('${PROJECT_NAME}')
	.config(${entity.name}Config);

	UfConfig.${dollarSymbol}inject = ['PlcMenuLoaderProvider']

	function UfConfig(PlcMenuLoaderProvider) {
		PlcMenuLoaderProvider.addMenuPath('app/${uncapitalizedEntityName}/${uncapitalizedEntityName}.menu.json');
	}

})();
